CREATE TABLE datos (
    Fila INT,
    Aplicacion VARCHAR(50),
    Estado VARCHAR(50)
);

INSERT INTO datos (Fila, Aplicacion, Estado) VALUES
(1, 'Web', 'Aprobado'),
(2, NULL, 'Fallo'),
(3, NULL, 'Fallo'),
(4, NULL, 'Fallo'),
(5, 'App', 'Aprobado'),
(6, NULL, 'Fallo'),
(7, NULL, 'Fallo'),
(8, NULL, 'Aprobado'),
(9, NULL, 'Aprobado'),
(10, 'RESTAPI', 'Fallo'),
(11, NULL, 'Fallo'),
(12, NULL, 'Fallo');

WITH RECURSIVE relleno AS (
    SELECT 
        Fila,
        Aplicacion,
        Estado
    FROM datos
    WHERE Fila = 1

    UNION ALL

    SELECT
        d.Fila,
        CASE 
            WHEN d.Aplicacion IS NULL THEN r.Aplicacion 
            ELSE d.Aplicacion 
        END AS Aplicacion,
        d.Estado
    FROM datos d
    JOIN relleno r ON d.Fila = r.Fila + 1
)
SELECT * FROM relleno
